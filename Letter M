using System;

namespace Atividade_4___01
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("vc anda faz parte do clube? ");
			string yeep = Convert.ToString(Console.ReadLine());
			
			if (yeep == "sim"){
				Console.WriteLine("Por favor, atualize sua inscrição para continuar usufruindo dos benefícios do clube");
			}
			else{
				Console.WriteLine("ok entt");
			}
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
